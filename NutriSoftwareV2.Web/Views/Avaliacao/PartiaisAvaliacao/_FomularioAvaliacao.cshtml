                    <div class="w-140px p-1 pl-1 float-left bg-secondary text-light">Peso</div>
                    <div class="float-left" style="width:100px;"><input type="text" asp-for="Peso" mask="formAval" class="form-control h32px w-140px" /></div>
                    <div style="clear:both;"></div>
                </div>
                <div class="row mt-1">
                    <div class="bg-secondary" style="width:100%;">
                        <p class="text-center m-1 text-light"><b>Circunferências</b></p>
                    </div>
                </div>
                <div class="row mt-1">
                    <div class="float-left ml-4" style="width:140px;"><input type="text" placeholder="@avaliacaAnterior.CircCintura" readonly class="form-control h32px w-140px avalAnterior" /></div>
                    <div class="w-140px p-1 pl-1 float-left bg-secondary text-light">Circ. Cintura</div>
