@using NutriSoftwareV2.Negocio.Domain
@model NutriSoftwareV2.Negocio.Domain.AvaliacaoFisica

@{
    AvaliacaoFisica avaliacaAnterior = ViewBag.AvaliacaoAnterior == null ? new AvaliacaoFisica() : ViewBag.AvaliacaoAnterior;
}

<div class="modal-content">
    <div class="modal-header">
        <h5 class="modal-title">Cadastro de avaliação</h5>
        <button type="submit" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">x</span>
        </button>
    </div>
    <form method="post" id="formularioCadastrarEditarAvaliacao" class="p-0">
        <div class="modal-body">
            <div class="container">
                <input type="hidden" asp-for="Id" id="avaliacaoId"/>
                <input type="hidden" asp-for="PacienteId" />
                <input type="hidden" name="IdPaciente" value="@ViewBag.IdPaciente" />
                <div class="row mt-1">
                    <div class="float-left ml-4" style="width:140px;"><input type="text" value="@avaliacaAnterior.NumAvaliacao" readonly class="form-control h32px w-140px avalAnterior" /></div>
                    <div class="w-140px p-1 pl-1 float-left bg-secondary text-light">Avaliacao</div>
                    <div class="float-left" style="width:100px;"><input type="text" asp-for="NumAvaliacao" class="form-control h32px w-140px" /></div>
                    <div style="clear:both;"></div>
                </div>
                <div class="row mt-1">
                    <div class="float-left ml-4" style="width:140px;"><input type="date" asp-for="@avaliacaAnterior.DataAvaliacao" readonly class="form-control h32px w-140px avalAnterior" /></div>
                    <div class="w-140px p-1 pl-1 float-left bg-secondary text-light">Data</div>
                    <div class="float-left" style="width:100px;"><input type="date" asp-for="DataAvaliacao" class="form-control h32px w-140px" /></div>
                    <div style="clear:both;"></div>
                </div>
                <div class="row mt-1">
