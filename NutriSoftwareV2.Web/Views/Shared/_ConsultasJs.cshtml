<script src="~/js/consulta.js"></script>

<script>
    function AbrirAba_Onclick(url,btnId,codigoConsulta,pacienteId) {
        MenuConsulta(url, btnId, codigoConsulta, 'pnlConteudoConsulta', pacienteId)
    }
    function InserirAnotacao_OnClick() {
        InserirAtualizarDados('@Url.Action("InserirEditarAnotacao", "Consulta")', 'frmObservacaoConsulta', 'pnlConteudoConsulta', 'Anotação pré inserida.', '2')
    }
    function AutoCompleteFieldAlimentos() {
        autoComplete('@Url.Action("AutoComplete", "Dieta")', 'txtAlimento', 'txtAlimentoId');
    }

    function AutoCompeteTipoMedidaAlimento() {
        autoComplete('@Url.Action("AutoCompleteTipoMedidas", "Dieta")', 'txtTpMedidaAlimento', 'txtTpMedidaAlimentoId');
    }

    function InserirAvaliacao_OnClick() {
        if (validaCamposAvaliacao()) { 
            InserirAtualizarDados('@Url.Action("InserirEditarAvaliacao", "Consulta")', 'frmCadastrarAvaliacaoConsulta', 'pnlConteudoConsulta', 'Avaliação pré inserida.', '2')
        }
    }

    
   
</script>
